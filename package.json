{
  "name": "cc-vauldt",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:cf": "pnpm run build:op && pnpm run dev",
    "build": "next build",
    "build:op": "opennextjs-cloudflare build",
    "schedule": "pnpm run build:op && wrangler dev --test-scheduled",
    "start": "next start",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "prepare": "husky",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "ai-check": "pnpm run typecheck && pnpm run lint",
    "preview": "opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare deploy",
    "deploy:staging": "opennextjs-cloudflare deploy -- --env=staging",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db:create": "wrangler d1 create cc-vault-db",
    "db:create:staging": "wrangler d1 create cc-vault-db-staging",
    "db:migrate": "wrangler d1 migrations apply cc-vault-db",
    "db:migrate:staging": "wrangler d1 migrations apply cc-vault-db-staging --env staging",
    "db:migrate:local": "wrangler d1 migrations apply cc-vault-db --local",
    "db:console:staging": "wrangler d1 execute cc-vault-db-staging --command --env staging"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.5.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "linkedom": "^0.18.11",
    "lucide-react": "^0.487.0",
    "next": "15.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "happy-dom": "^17.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.25.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "",
    "*.{js,jsx,ts,tsx,json,css,md}": ""
  }
}
